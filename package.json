{
  "name": "astro-loader-storyblok",
  "version": "1.0.1",
  "description": "Astro content loader for Storyblok",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "LICENSE.txt",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc",
    "dev": "tsc --watch",
    "debug:playground": "DEBUG=astro:loader-storyblok* pnpm --filter ./playground/minimal-astro-project dev",
    "debug:playground:force": "pnpm debug:playground --force",
    "prepublishOnly": "pnpm build",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "lint:markdown": "markdownlint-cli2 \"**/*.{md,mdx}\" \"!node_modules\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "astro",
    "storyblok",
    "withastro",
    "astro-component",
    "astro-loader"
  ],
  "author": "Romain Pironneau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/romainpi/astro-loader-storyblok.git",
    "branch": "main"
  },
  "homepage": "https://github.com/romainpi/astro-loader-storyblok#readme",
  "bugs": {
    "url": "https://github.com/romainpi/astro-loader-storyblok/issues"
  },
  "packageManager": "pnpm@10.18.2+sha256.93421022fe0156d7cbdf699edc3d7eab39654986eac14f927d02b85f0dac6d61",
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@github/markdownlint-github": "0.8.0",
    "@types/node": "24.7.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "astro": "5.14.4",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "happy-dom": "20.0.0",
    "markdown-it": "14.1.0",
    "markdownlint-cli2": "0.18.1",
    "markdownlint-cli2-formatter-pretty": "0.0.8",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@storyblok/js": "4.3.2",
    "@storyblok/management-api-client": "0.1.5"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  }
}
