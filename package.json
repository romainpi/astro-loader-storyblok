{
  "name": "astro-loader-storyblok",
  "version": "0.2.0",
  "description": "Storyblok loader for the Astro Content Layer API.",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "LICENSE.txt",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --fix",
    "type-check": "tsc --noEmit",
    "lint:markdown": "markdownlint-cli2 \"**/*.{md,mdx}\" \"!node_modules\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "astro",
    "storyblok",
    "withastro",
    "astro-loader"
  ],
  "author": "Romain Pironneau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/romainpi/astro-loader-storyblok.git",
    "branch": "main"
  },
  "homepage": "https://github.com/romainpi/astro-loader-storyblok#readme",
  "bugs": {
    "url": "https://github.com/romainpi/astro-loader-storyblok/issues"
  },
  "packageManager": "pnpm@10.17.0+sha256.bd9ec5417641391e0acb3912b2912bd5b0385407a82da94744a7407b567fdb4f",
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@github/markdownlint-github": "0.8.0",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "astro": "5.13.10",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "happy-dom": "18.0.1",
    "markdown-it": "14.1.0",
    "markdownlint-cli2": "0.18.1",
    "markdownlint-cli2-formatter-pretty": "0.0.8",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@storyblok/js": "4.3.0",
    "@storyblok/management-api-client": "0.1.4"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  }
}
