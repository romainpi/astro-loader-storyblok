{
  "name": "astro-loader-storyblok",
  "version": "0.2.6",
  "description": "Astro content loader for Storyblok",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "LICENSE.txt",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc",
    "prepublishOnly": "pnpm build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --fix",
    "type-check": "tsc --noEmit",
    "lint:markdown": "markdownlint-cli2 \"**/*.{md,mdx}\" \"!node_modules\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "astro",
    "storyblok",
    "withastro",
    "astro-component",
    "astro-loader"
  ],
  "author": "Romain Pironneau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/romainpi/astro-loader-storyblok.git",
    "branch": "main"
  },
  "homepage": "https://github.com/romainpi/astro-loader-storyblok#readme",
  "bugs": {
    "url": "https://github.com/romainpi/astro-loader-storyblok/issues"
  },
  "packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa",
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@github/markdownlint-github": "0.8.0",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "astro": "5.14.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "happy-dom": "19.0.1",
    "markdown-it": "14.1.0",
    "markdownlint-cli2": "0.18.1",
    "markdownlint-cli2-formatter-pretty": "0.0.8",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@storyblok/js": "4.3.0",
    "@storyblok/management-api-client": "0.1.4"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  }
}
